<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="groupProject01.SettingsPage"
             xmlns:local="clr-namespace:groupProject01"
             Title="Settings">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout VerticalOptions="StartAndExpand">
                <Button x:Name="Logout" Text="Logout" Clicked="OnLogout" />
                <Button x:Name="CurApartment" Text="Current Apartment" Clicked="OnCurApartment" />
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>