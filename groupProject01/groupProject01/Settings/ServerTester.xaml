<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="groupProject01.Settings.ServerTester">
  <ScrollView>
    <StackLayout VerticalOptions="StartAndExpand">
      <Editor x:Name ="myText">
      </Editor>



      <Button x:Name="Save" Text="Save" Clicked="onSendList" />
      <Button x:Name="Retrieve" Text="Retrieve" Clicked="onGetList" />
      <Button x:Name="Lists" Text="Lists" Clicked="onSendEvent" />
      <Button x:Name="Settings" Text="Settings" Clicked="onGetCal" />
      <Button x:Name="Calendar" Text="Calendar" Clicked="onSendMessage" />
      <Button x:Name="Messaging" Text="Messaging" Clicked="onGetMessage" />
      <Button x:Name="Home" Text="Home" Clicked="OnHome" />
      <Button x:Name="Login" Text="Login" Clicked="OnLogin" />
    </StackLayout>
  </ScrollView>
</ContentPage>